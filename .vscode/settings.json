    "**/{desktop.ini,node_modules,*.lnk,*.url}": true,
    "**/*.{7z,ico,jpeg,jpg,pdf,pdn,png,sign,so,svg,unknown}": true,
    "**/~$*.{xls,xlsx,doc,docx}": true
  },
  "files.hotExit": "off",
  "files.watcherExclude": {
    "**/.*": true
  },
